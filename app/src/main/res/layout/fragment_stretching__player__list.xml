<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".fragment.Stretching_Player_Fragment">

    <WebView
        android:id="@+id/webView"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
    />
    <LinearLayout
        android:id="@+id/linear"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="10dp"
        android:orientation="vertical">
        <TextView
            android:layout_marginBottom="20dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textColor="@color/black"
            android:textSize="10pt"
            android:layout_marginTop="30dp"
            android:text="스트레칭 Player"
            android:fontFamily="@font/gmarketsans_medium"/>

        <ImageView
            android:id="@+id/ddangs_one"
            android:layout_margin="10dp"
            android:src="@drawable/ddangs_one"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>

        <ImageView
            android:id="@+id/ddangs_two"
            android:layout_margin="10dp"
            android:src="@drawable/ddangs_two"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>



    </LinearLayout>
</FrameLayout>